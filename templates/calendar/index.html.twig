{% extends 'base.html.twig' %}

{% block title %}Hello CalendarController!{% endblock %}

{% block body %}
<div class="container">
    <div class="row">
        <table class="table">
        <caption >September</caption>
        <thead>
            <tr>
            <th scope="col">Mon</th>
            <th scope="col">Tue</th>
            <th scope="col">Wed</th>
            <th scope="col">Thu</th>
            <th scope="col">Fri</th>
            <th scope="col">Sat</th>
            <th scope="col">Sun</th>
            </tr>
        </thead>
        <tbody>
            {% for  i in 0..5 %}
                <tr id="{{i}}">
                {% for j in 1..7 %}
                    <td id="{{i}}{{j}}">{{i}}{{j}}</td>
                {% endfor %}
                </tr>
            {% endfor %}
        </tbody>
        </table>
    </div>
</div>
{% endblock %}
